<!-- popups -->

<!-- popup login -->
<div id="pop-login" class="modal_div">
    <h2>Вход в систему</h2>
    <form action="/login/start" method="post">
        <label for="poplog-inp1">E-mail</label>
        <div class="c-inp-err-t"></div>
        <input type="text" id="poplog-inp1" required="required" name="email">
        <label for="poplog-inp2">Пароль</label>
        <div class="c-inp-err-t"></div>
        <input type="password" id="poplog-inp2" required="required" name="password">
        <input type="submit" value="Войти">
        <div class="link-login">
            <span> <a href="#pop-start" class="open_modal" onclick="$('#pop-login').hide()">Регистрация нового пользователя</a><a href="#pop-recovery" class="open_modal" onclick="$('#pop-login').hide()">Забыли пароль?</a></span>
        </div>
    </form>

    <div class="modal-close"></div>
</div>

<!-- popup callback -->
<div id="pop-callback" class="modal_div">
    <h2>Заказать звонок</h2>
    <form action="/register/callback" method="post">
        <label for="poplog-inp1">Телефон</label>
        <div class="c-inp-err-t"></div>
        <input type="text" id="poplog-inp1" required="required" name="phone">
        <input type="submit" value="Отправить">
    </form>
    <div class="modal-close"></div>
</div>

<!-- popup start -->
 <div id="pop-start" class="modal_div">
    <h2>Регистрация</h2>
    <form class="reg-new-user" method="POST">
<!--        <label for="popstrt-inp1">Ваше имя</label>-->
<!--        <input type="text" id="popstrt-inp1" required="required" name="name">-->
        <label class="fontNormal" for="popstrt-inp1">E-mail</label>
        <div class="c-inp-err-t"></div>
        <input type="text" id="popstrt-inp1" required="required" name="email">
<!--        <label for="popstrt-inp1">Телефон</label>-->
<!--        <input type="text" id="popstrt-inp1" required="required" name="phone">-->
        <label class="fontNormal" for="popstrt-inp1">Пароль</label>
        <div class="c-inp-err-t"></div>
        <input type="password" id="popstrt-inp1" required="required" name="password">
        <label class="fontNormal" for="popstrt-inp1">Повторите пароль</label>
        <div class="c-inp-err-t"></div>
        <input type="password" id="popstrt-inp1" required="required" name="confpassword">
