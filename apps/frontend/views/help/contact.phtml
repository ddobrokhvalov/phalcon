<div class="content">
    <script type="text/javascript" src="/js/input_mask.js"></script>
    <div class="wrapper">
        <div class="c-content">
            <style>
                .wrap_success{
                    width: 100%;
                    text-align: center;
                    top: 58px;
                    position: absolute;
                }

                .c-m-form input[type="email"] {
                    background: #fff none repeat scroll 0 0;
                    border: 1px solid #d4d7df;
                    border-radius: 2px;
                    float: left;
                    height: 55px;
                    margin: 0 0 30px;
                    padding: 0 10px;
                    width: 100%;
                }
            </style>
            <div class="wrap_success">
                {{ flashSession.output() }}
            </div>

            <div class="c-cnt-pagename">
                Контакты
            </div>

            <div class="c-mapb">
                <script type="text/javascript" charset="utf-8" src="https://api-maps.yandex.ru/services/constructor/1.0/js/?sid=S6PvMetJxzVzDK2kZx6n9rfkbBu45eCR&width=970&height=470&lang=ru_UA&sourceType=constructor"></script>

            </div>            
            <div class="c-m-form">
                <span>Напишите нам</span>
                <form action="/help/" method="POST" id="contact-form">
                    <label for="cmf1">Как вас зовут</label>
                    <input type="text" id="cmf1" name="sender-name">
                    <label for="cmf2">Контактный телефон</label>
                    <input type="text" id="cmf2" name="sender-phone" class="phone">
                    <label for="cmf3" >E-mail</label>
                    <input type="email" id="cmf3" name="sender-email">
                    <label for="cmf4">Ваше собщение</label>
                    <textarea id="cmf4" name="sender-message"></textarea>
                    <input type="submit" value="Отправить сообщение">
                </form>
            </div>

            <div class="c-m-addr">
                <span>Наш офис</span>
                109097, СанктПетербург, ул. Ильинка, 7<br>
                Skype: Goszakaz<br>
                E-mail: zakupki_uis@roskazna.ru<br>
                Телефон: 7-495-2147529
            </div>


        </div>
    </div>
</div>
<script>
    $(document).ready(function() {
        $(".phone").mask("+7 (999) 999-9999");
    });
</script>